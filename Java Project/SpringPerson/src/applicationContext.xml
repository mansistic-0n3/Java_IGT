<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns = "http://www.springframework.org/schema/beans" 
xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	<bean id="per" class="com.Person">
		<property name="pid" value="100"></property>
		<property name="name" value="Mansi"></property>
		
		<property name="hobbies">
			<list>
				<value>C</value>
				<value>C++</value>
				<value>C#</value>
				<value>Java</value>
				<value>Php</value>
				<value>Web</value>
			</list>
		</property>
		
		<property name="emails">
			<list>
				<value>mansi@gmail.com</value>
				<value>chaurasia@gmail.com</value>
			</list>
		</property>
		
		<property name="phones">
			<set>
				<value>1234567890</value>
				<value>7890123456</value>
				<value>9012345678</value>
			</set>
		</property>
		
		<property name="education">
			<map>
				<entry key="graduation" value="BCA"></entry>
				<entry key="postgraduation" value="MCA"></entry>
			</map>
		</property>

    </bean>
</beans>